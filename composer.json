    },
    "extra": {
        "laravel": {
            "providers": [
                "Aginev\\Datagrid\\DatagridServiceProvider"
            ],
            "aliases": {
                "Datagrid": "Aginev\\Datagrid\\Datagrid"
            }
        }
    }
}
