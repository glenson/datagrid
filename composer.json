{
    "name": "aginev/datagrid",
    "description": "Datagrid package for Laravel v5",
	"keywords": ["Datagrid", "Datagrid Package for Laravel v5"],
    "homepage": "http://aginev.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Atanas Ginev",
            "email": "naskospam@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
